#include <stdio.h>
#include <stdlib.h>
#include <time.h>


int main() {
    srand(time(NULL)); 
        int valor1 = rand() % 13 + 1;  
        int valor2 = rand() % 13 + 1; 
        int palo1 = rand() % 4;        
        int palo2 = rand() % 4;
        
        
        if (valor1== 1)
        printf("A");
        else if (valor1==11)
        printf("J");
        else if (valor1==12)
        printf("Q");
        else if (valor1==13)
        printf("K");
        else 
        printf("[%d]",valor1);
        
        
        if (palo1==0)
        printf("C");
        else if (palo1==1)
        printf("T");
        else if (palo1==2)
        printf("P");
        else
        printf("D");
        
        
        if (valor2== 1)
        printf("A");
        else if (valor2==11)
        printf("J");
        else if (valor2==12)
        printf("Q");
        else if (valor2==13)
        printf("K");
        else 
        printf("[%d]",valor2);
        
        if (palo2==0)
        printf("C");
        else if (palo2==1)
        printf("T");
        else if (palo2==2)
        printf("P");
        else
        printf("D");

        if (valor1==valor2)
        {
        printf("\n valores iguales TRUE");
        } 
        else
        {
        printf("\n valores iguales FALSE");
        }


    return 0;
}
